#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/bin/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/bin/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/next@16.1.4_@babel+core@7.28.6_@playwright+test@1.58.0_babel-plugin-react-compiler@1.0._224705b02ff49d6c565529e718589fe3/node_modules/next/dist/bin/next" "$@"
fi
