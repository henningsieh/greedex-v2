#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/cuid2/bin/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/cuid2/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/cuid2/bin/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/cuid2/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules/@paralleldrive/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/@paralleldrive+cuid2@3.0.6/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@paralleldrive/cuid2/bin/cuid2.js" "$@"
else
  exec node  "$basedir/../@paralleldrive/cuid2/bin/cuid2.js" "$@"
fi
