#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules/fumadocs-mdx/dist/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules/fumadocs-mdx/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules/fumadocs-mdx/dist/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules/fumadocs-mdx/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/fumadocs-mdx@11.10.1_fumadocs-core@15.8.5_@types+react@19.2.9_lucide-react@0.563.0_reac_a5732cea618dfbec78a470c46ef5040a/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
else
  exec node  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
fi
