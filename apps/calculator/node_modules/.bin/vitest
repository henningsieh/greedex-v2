#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/vitest@4.0.18_@types+node@25.0.10_@vitest+ui@4.0.18_jiti@2.6.1_jsdom@27.4.0_@noble+hash_28b7f22e4696a2ef894ea4e08cc523d5/node_modules/vitest/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/vitest@4.0.18_@types+node@25.0.10_@vitest+ui@4.0.18_jiti@2.6.1_jsdom@27.4.0_@noble+hash_28b7f22e4696a2ef894ea4e08cc523d5/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/vitest@4.0.18_@types+node@25.0.10_@vitest+ui@4.0.18_jiti@2.6.1_jsdom@27.4.0_@noble+hash_28b7f22e4696a2ef894ea4e08cc523d5/node_modules/vitest/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/vitest@4.0.18_@types+node@25.0.10_@vitest+ui@4.0.18_jiti@2.6.1_jsdom@27.4.0_@noble+hash_28b7f22e4696a2ef894ea4e08cc523d5/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
