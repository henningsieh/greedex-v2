#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules/ts-prune/lib/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules/ts-prune/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules/ts-prune/lib/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules/ts-prune/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/ts-prune@0.10.3/node_modules:/home/runner/work/greedex-calculator/greedex-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-prune/lib/index.js" "$@"
else
  exec node  "$basedir/../ts-prune/lib/index.js" "$@"
fi
